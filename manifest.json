{
  "manifest_version": 2,

  "name": "Dev CC",
  "description": "Generates a valid credit card number for development use.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Generate CC"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
